{
  "name": "jonwaldstein/vibrato",
  "type": "wordpress-theme",
  "license": "MIT",
  "description": "A Wordpress Theme Boilerplate",
  "homepage": "https://github.com/jonwaldstein/vibrato",
  "authors": [
    {
      "name": "Jon Waldstein",
      "email": "jpwaldstein@gmail.com",
      "homepage": "https://github.com/jonwaldstein/vibrato"
    }
  ],
  "keywords": [
    "wordpress"
  ],
  "support": {
    "issues": "https://github.com/jonwaldstein/vibrato/issues"
  },
  "require": {
    "php": ">=7.4.0",
    "composer/installers": "~1.0",
    "htmlburger/carbon-fields": "^3.2",
    "spatie/enum": "^3.7"
  },
  "config": {
    "optimize-autoloader": true,
    "allow-plugins": {
      "composer/installers": true
    }
  },
  "autoload": {
    "psr-4": {
        "Vibrato\\": "app/"
    },
    "classmap": [
        "app/"
    ]
  }
}
